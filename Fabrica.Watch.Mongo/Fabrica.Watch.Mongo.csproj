		<PackageReference Include="AWSSDK.SecurityToken" Version="3.7.103.14" ExcludeAssets="All" />
		<PackageReference Include="AWSSDK.Core" Version="3.7.107.6" ExcludeAssets="All" />
	</ItemGroup>


	<ItemGroup>
	  <ProjectReference Include="..\Fabrica.Watch\Fabrica.Watch.csproj" />
